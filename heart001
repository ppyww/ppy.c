#include <stdio.h>
#include <stdlib.h>
#include <windows.h>

int main(int argc,char*argv[])
{
    float x,y,a;
    for(x=-1.5;x<1.5;x+1.5){
    	for(y=1.5;y>-1.5;y-=0.1){
	    a=x*x+y*y-1;
	    putchar(a*a*a-x*x*y*y*y<=0.0?'@':' ');
	}
	 system("color 0c");
	 putchar('\n');
  }
      printf ("hhhhh");
       return 0;
}
